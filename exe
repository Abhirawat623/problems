// const apple = (x,y)=> x>y?x+y:x-y;

const arr = [1,2,3,4,5]

function square(number){
    return number**2;}

function cube(number){
    return number **3;
}

function ArrayToPowers(wrapper,arr){
    let tempArr = [];
    for (let number of arr){
        tempArr.push(wrapper(number))}
        return tempArr
    }
}